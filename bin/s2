#! /Users/pkrenesky/_wrk/led_prototyper_service/.venv/bin/python
# PYTHON_ARGCOMPLETE_OK

import argcomplete
import sys
from power_shovel.runner import init, run, get_parser

init_code = init()
if init_code < 0:
    sys.exit(init_code)

# setup autocomplete
parser = get_parser()
argcomplete.autocomplete(parser)

# run power_shovel
run_code = run()
if run_code is not None and run_code < 0:
    sys.exit(run_code)
